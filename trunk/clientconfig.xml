<?xml version="1.0" ?>
<NcvfsConfig>
    <Communication>
		<NumClientThreads>10</NumClientThreads>
    </Communication>
    
    <Storage>
    	<!-- KB -->
    	<ObjectSize>10240</ObjectSize>
    </Storage>

	<Fuse>
		<PreallocateObjectNumber>10</PreallocateObjectNumber>
		<TempStorage>/tmp/</TempStorage>
		<ReadAhead>10</ReadAhead>
		<WritePoolLimit>20</WritePoolLimit>
		<ReadCacheLimit>50</ReadCacheLimit>
	</Fuse>
</NcvfsConfig>
